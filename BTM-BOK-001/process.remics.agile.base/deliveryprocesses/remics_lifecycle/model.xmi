        <presentation xsi:type="org.eclipse.epf.uma:DescriptorDescription" xmi:id="-RwUAf6Bo7OphDy8tk776xQ" name="requirements_model,_LZGG4gpFEeOBM7QWB5hbCw" guid="-RwUAf6Bo7OphDy8tk776xQ">
