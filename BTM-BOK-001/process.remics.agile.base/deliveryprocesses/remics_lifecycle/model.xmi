        <processElements xsi:type="org.eclipse.epf.uma:RoleDescriptor" xmi:id="_Ak3bwQDjEeOBVZ2G27rUkQ" name="scrum_team" guid="_Ak3bwQDjEeOBVZ2G27rUkQ" presentationName="Scrum Team" briefDescription="The Scrum Team consists of a Product Owner, the Development Team, and a Scrum Master." isPlanned="false" superActivities="_zBguEf25EeK8HINtOGo1Xg" Role="_DRxwUM3SEeKOw8KXk47y0g" responsibleFor="_UpOuAQvQEeOhutvlRKZMkg _UpRKQQvQEeOhutvlRKZMkg _UpTmgQvQEeOhutvlRKZMkg">
