  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-qDzsIpWNjcPNAkr4zBBuGg" name="uam_define_ad,_mESOMMhUEdyU89e0dKAsrw" guid="-qDzsIpWNjcPNAkr4zBBuGg">
    <usageGuidance>&lt;p>&#xD;
    The following is provided to aid in deciding if a decision is architecturally significant, or if it is simply a design&#xD;
    decision.&#xD;
&lt;/p>&#xD;
&lt;p align=&quot;center&quot;>&#xD;
    &lt;strong>Table – Architectural Decisions vs. Design Decisions&lt;/strong>&#xD;
&lt;/p>&#xD;
&lt;div align=&quot;center&quot;>&#xD;
    &lt;table class=&quot;MsoTableGrid&quot;&#xD;
    style=&quot;BORDER-TOP: medium none; BORDER-RIGHT: medium none; BORDER-COLLAPSE: collapse; BORDER-BOTTOM: medium none; BORDER-LEFT: medium none; mso-border-alt: solid black .5pt; mso-border-themecolor: text1; mso-yfti-tbllook: 1184; mso-padding-alt: 1.45pt 5.75pt 1.45pt 5.75pt&quot;&#xD;
     cellspacing=&quot;0&quot; cellpadding=&quot;0&quot; border=&quot;1&quot;>&#xD;
        &lt;thead>&#xD;
            &lt;tr style=&quot;mso-yfti-irow: 0; mso-yfti-firstrow: yes&quot;>&#xD;
                &lt;td&#xD;
                style=&quot;BORDER-TOP: black 1pt solid; BORDER-RIGHT: black 1pt solid; WIDTH: 222.85pt; BACKGROUND: #4f81bd; BORDER-BOTTOM: black 1pt solid; PADDING-BOTTOM: 1.45pt; PADDING-TOP: 1.45pt; PADDING-LEFT: 5.75pt; BORDER-LEFT: black 1pt solid; PADDING-RIGHT: 5.75pt; mso-border-alt: solid black .5pt; mso-border-themecolor: text1; mso-background-themecolor: accent1&quot;&#xD;
                 valign=&quot;top&quot; width=&quot;297&quot;>&#xD;
                    &lt;p class=&quot;BodyTextTableHeader&quot; style=&quot;MARGIN: 2pt 0in&quot;>&#xD;
                        &lt;strong>&lt;font color=&quot;#ffffff&quot;>Architectural Decision&lt;br />&#xD;
                        &lt;/font>&lt;font color=&quot;#ffffff&quot;>(Must be documented using the AD Process)&lt;/font>&lt;/strong>&#xD;
                    &lt;/p>&#xD;
                &lt;/td>&#xD;
                &lt;td&#xD;
                style=&quot;BORDER-TOP: black 1pt solid; BORDER-RIGHT: black 1pt solid; WIDTH: 222.85pt; BACKGROUND: #4f81bd; BORDER-BOTTOM: black 1pt solid; PADDING-BOTTOM: 1.45pt; PADDING-TOP: 1.45pt; PADDING-LEFT: 5.75pt; BORDER-LEFT: #f0f0f0; PADDING-RIGHT: 5.75pt; mso-border-alt: solid black .5pt; mso-border-themecolor: text1; mso-background-themecolor: accent1; mso-border-left-alt: solid black .5pt; mso-border-left-themecolor: text1&quot;&#xD;
                 valign=&quot;top&quot; width=&quot;297&quot;>&#xD;
                    &lt;p class=&quot;BodyTextTableHeader&quot; style=&quot;MARGIN: 2pt 0in&quot;>&#xD;
                        &lt;strong>&lt;font color=&quot;#ffffff&quot;>Design Decision&lt;br />&#xD;
                        &lt;/font>&lt;font color=&quot;#ffffff&quot;>(Not documented using the AD Process)&lt;/font>&lt;/strong>&#xD;
                    &lt;/p>&#xD;
                &lt;/td>&#xD;
            &lt;/tr>&#xD;
        &lt;/thead>&#xD;
        &lt;tbody>&#xD;
            &lt;tr style=&quot;mso-yfti-irow: 1&quot;>&#xD;
                &lt;td&#xD;
                style=&quot;BORDER-TOP: #f0f0f0; BORDER-RIGHT: black 1pt solid; WIDTH: 222.85pt; BORDER-BOTTOM: black 1pt solid; PADDING-BOTTOM: 1.45pt; PADDING-TOP: 1.45pt; PADDING-LEFT: 5.75pt; BORDER-LEFT: black 1pt solid; PADDING-RIGHT: 5.75pt; BACKGROUND-COLOR: transparent; mso-border-alt: solid black .5pt; mso-border-themecolor: text1; mso-border-top-alt: solid black .5pt; mso-border-top-themecolor: text1&quot;&#xD;
                 valign=&quot;top&quot; width=&quot;297&quot;>&#xD;
                    &lt;p class=&quot;BodyTextTable&quot; style=&quot;MARGIN: 0in 0in 2pt&quot;>&#xD;
