      <processElements xsi:type="org.eclipse.epf.uma:RoleDescriptor" xmi:id="_U2hjIQvQEeOhutvlRKZMkg" name="scrum_team" guid="_U2hjIQvQEeOhutvlRKZMkg" presentationName="Scrum Team" briefDescription="The Scrum Team consists of a Product Owner, the Development Team, and a Scrum Master." isPlanned="false" superActivities="_1kHlwTO7EeK5hZv7WCtlvg" Role="_O65KUOF6Edyp34pwdTOSVQ" responsibleFor="_UzOXgQvQEeOhutvlRKZMkg _UzAVEQvQEeOhutvlRKZMkg _UzYvkQvQEeOhutvlRKZMkg">
