        <processElements xsi:type="org.eclipse.epf.uma:WorkProductDescriptor" xmi:id="_mjKFFf27EeK8HINtOGo1Xg" name="additional_system_requirements" guid="_mjKFFf27EeK8HINtOGo1Xg" presentationName="Additional System Requirements" briefDescription="The Additional System Requirements contains the list of new or modified features that are requested to be added to the new system" isPlanned="false" superActivities="_BElx0f27EeK8HINtOGo1Xg" WorkProduct="_FxNEQV5BEeKVU8eUCvi3Og" deliverableParts="_mjRZ0v27EeK8HINtOGo1Xg _mjRZ1f27EeK8HINtOGo1Xg _32bm0QD0EeOBVZ2G27rUkQ">
