      <processElements xsi:type="org.eclipse.epf.uma:WorkProductDescriptor" xmi:id="_UjkK4QvQEeOhutvlRKZMkg" name="future_deployment" guid="_UjkK4QvQEeOhutvlRKZMkg" presentationName="Future Deployment" briefDescription="The Future deployment is a part of the system overview, it describes the expected future deployment of the system." isPlanned="false" superActivities="_xsG88TO8EeK5hZv7WCtlvg">
        <methodElementProperty name="descriptor_createdByReference" value="true"/>
        <methodElementProperty name="me_references" value="&lt;?xml version=&quot;1.0&quot; encoding=&quot;UTF-8&quot; standalone=&quot;no&quot;?>&lt;me_references udtList=&quot;&quot;/>"/>
        <presentation xsi:type="org.eclipse.epf.uma:DescriptorDescription" xmi:id="-vr7_S60tIHmgalQCjoZVqA" name="future_deployment,_UjkK4QvQEeOhutvlRKZMkg" guid="-vr7_S60tIHmgalQCjoZVqA">
          <refinedDescription>&lt;h3>&#xD;
    Future Deployment&#xD;
&lt;/h3>&#xD;
&lt;p>&#xD;
    The future deployment describes de location of the main parts of the future system in the public and private computing&#xD;
    infrastructures.&#xD;
&lt;/p></refinedDescription>
        </presentation>
        <WorkProduct href="uma://_4BH4wPNGEeGUHqJDPoQpuw#_mE4_QP28EeK8HINtOGo1Xg"/>
