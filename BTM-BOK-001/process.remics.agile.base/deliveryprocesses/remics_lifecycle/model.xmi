      <processElements xsi:type="org.eclipse.epf.uma:RoleDescriptor" xmi:id="_LZFf0QpFEeOBM7QWB5hbCw" name="product_owner" guid="_LZFf0QpFEeOBM7QWB5hbCw" presentationName="Product Owner" briefDescription="The Product Owner represents the interests of all stakeholders, defines the features of the product and manages the Product Backlog." isPlanned="false" superActivities="_tzY-8TO8EeK5hZv7WCtlvg" Role="_tvQEgQmREeOmqbz9Sywm-g" responsibleFor="_QKUkwQpGEeOBM7QWB5hbCw">
