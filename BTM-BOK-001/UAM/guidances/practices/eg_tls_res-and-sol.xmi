    name=&quot;map1392831077277&quot;>&#xD;
        &lt;area title=&quot;Research and Solutions Applications Server&quot;&#xD;
        href=&quot;./../../../UAM/guidances/practices/eg_tls_rands-server_11EF1EBE.html&quot; shape=&quot;rect&quot;&#xD;
