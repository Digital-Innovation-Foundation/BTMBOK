        &lt;tr style=&quot;HEIGHT: 19.6pt; mso-yfti-irow: 0; mso-yfti-firstrow: yes&quot;>
            &lt;td
            style=&quot;BORDER-TOP: gray 1pt solid; HEIGHT: 19.6pt; BORDER-RIGHT: gray 1pt solid; WIDTH: 462.2pt; BACKGROUND: #d9d9d9; BORDER-BOTTOM: gray 1pt solid; PADDING-BOTTOM: 0cm; PADDING-TOP: 0cm; PADDING-LEFT: 5.4pt; BORDER-LEFT: gray 1pt solid; PADDING-RIGHT: 5.4pt; mso-border-alt: solid gray .25pt&quot;
