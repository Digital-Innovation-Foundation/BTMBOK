      <processElements xsi:type="org.eclipse.epf.uma:WorkProductDescriptor" xmi:id="_92nTBAl4EeOmqbz9Sywm-g" name="validation_criteria" guid="_92nTBAl4EeOmqbz9Sywm-g" presentationName="Validation Criteria" briefDescription="The validation criteria is used to check the implementation of the requirements" isPlanned="false" superActivities="_qFoTwTO8EeK5hZv7WCtlvg" WorkProduct="_0zpAsADzEeOBVZ2G27rUkQ">
