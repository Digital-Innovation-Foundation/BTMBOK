  <org.eclipse.epf.uma:ActivityDescription xmi:id="-7WJyK7YMPxmuaa-T5jYJag" name="ea_bus_arch_review,_wC5V0EP4EeShYIpiBstbOQ" guid="-7WJyK7YMPxmuaa-T5jYJag">
