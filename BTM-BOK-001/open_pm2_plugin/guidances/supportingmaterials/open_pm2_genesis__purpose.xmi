&lt;P class=PM2-BulletList style=&quot;MARGIN: 0cm 0cm 0pt 35.45pt; TEXT-INDENT: -14.15pt; mso-list: l1 level1 lfo1&quot;>&lt;SPAN lang=EN-GB style=&quot;FONT-FAMILY: Symbol; mso-fareast-font-family: Symbol; mso-ansi-language: EN-GB; mso-bidi-font-family: Symbol&quot;>&lt;SPAN style=&quot;mso-list: Ignore&quot;>&lt;FONT size=3>·&lt;/FONT>&lt;SPAN style='FONT: 7pt &quot;Times New Roman&quot;'>&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;/SPAN>&lt;/SPAN>&lt;/SPAN>&lt;SPAN lang=EN-GB style=&quot;mso-ansi-language: EN-GB&quot;>&lt;FONT face=Calibri>&lt;FONT size=3>enabling better project management, leading to improved cost/effort efficiency&lt;o:p>&lt;/o:p>&lt;/FONT>&lt;/FONT>&lt;/SPAN>&lt;/P>
&lt;P class=PM2-BulletList style=&quot;MARGIN: 0cm 0cm 0pt 35.45pt; TEXT-INDENT: -14.15pt; mso-list: l1 level1 lfo1&quot;>&lt;SPAN lang=EN-GB style=&quot;FONT-FAMILY: Symbol; mso-fareast-font-family: Symbol; mso-ansi-language: EN-GB; mso-bidi-font-family: Symbol&quot;>&lt;SPAN style=&quot;mso-list: Ignore&quot;>&lt;FONT size=3>·&lt;/FONT>&lt;SPAN style='FONT: 7pt &quot;Times New Roman&quot;'>&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;/SPAN>&lt;/SPAN>&lt;/SPAN>&lt;SPAN lang=EN-GB style=&quot;mso-ansi-language: EN-GB&quot;>&lt;FONT face=Calibri>&lt;FONT size=3>enabling the improved monitor and control of European Union-funded projects and grants&lt;o:p>&lt;/o:p>&lt;/FONT>&lt;/FONT>&lt;/SPAN>&lt;/P>
&lt;P class=PM2-BulletList style=&quot;MARGIN: 0cm 0cm 0pt 35.45pt; TEXT-INDENT: -14.15pt; mso-list: l1 level1 lfo1&quot;>&lt;SPAN lang=EN-GB style=&quot;FONT-FAMILY: Symbol; mso-fareast-font-family: Symbol; mso-ansi-language: EN-GB; mso-bidi-font-family: Symbol; mso-bidi-font-style: italic&quot;>&lt;SPAN style=&quot;mso-list: Ignore&quot;>&lt;FONT size=3>·&lt;/FONT>&lt;SPAN style='FONT: 7pt &quot;Times New Roman&quot;'>&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;/SPAN>&lt;/SPAN>&lt;/SPAN>&lt;SPAN lang=EN-GB style=&quot;mso-ansi-language: EN-GB&quot;>&lt;FONT size=3>&lt;FONT face=Calibri>applying the European Commission decision of 12 December 2011 (2011/833/EU) on the &lt;I>“reuse of Commission documents to promote accessibility and reuse”.&lt;o:p>&lt;/o:p>&lt;/I>&lt;/FONT>&lt;/FONT>&lt;/SPAN>&lt;/P>
&lt;H3 style=&quot;MARGIN: 6pt 0cm 0pt 36pt; mso-list: l0 level3 lfo2&quot;>&lt;a name=&quot;_Toc526260393&quot;>&lt;SPAN lang=EN-GB style=&quot;mso-fareast-font-family: Calibri; mso-bidi-font-family: Calibri&quot;>&lt;SPAN style=&quot;mso-list: Ignore&quot;>&lt;FONT size=3 face=Calibri>1.5.1&lt;/FONT>&lt;SPAN style='FONT: 7pt &quot;Times New Roman&quot;'>&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;/SPAN>&lt;/SPAN>&lt;/SPAN>&lt;SPAN lang=EN-GB>&lt;FONT size=3 face=Calibri>Open PM² Publications&lt;/FONT>&lt;/SPAN>&lt;/a>&lt;SPAN lang=EN-GB>&lt;FONT size=3>&lt;FONT face=Calibri> &lt;o:p>&lt;/o:p>&lt;/FONT>&lt;/FONT>&lt;/SPAN>&lt;/H3>
