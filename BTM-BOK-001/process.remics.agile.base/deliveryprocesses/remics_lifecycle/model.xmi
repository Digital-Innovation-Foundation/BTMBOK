        <processElements xsi:type="org.eclipse.epf.uma:WorkProductDescriptor" xmi:id="_Uoj_oQvQEeOhutvlRKZMkg" name="future_deployment" guid="_Uoj_oQvQEeOhutvlRKZMkg" presentationName="Future Deployment" briefDescription="The Future deployment is a part of the system overview, it describes the expected future deployment of the system." isPlanned="false">
          <methodElementProperty name="descriptor_createdByReference" value="true"/>
          <methodElementProperty name="me_references" value="&lt;?xml version=&quot;1.0&quot; encoding=&quot;UTF-8&quot; standalone=&quot;no&quot;?>&lt;me_references udtList=&quot;&quot;/>"/>
