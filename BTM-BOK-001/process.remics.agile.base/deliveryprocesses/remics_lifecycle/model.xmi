          <presentation xsi:type="org.eclipse.epf.uma:DescriptorDescription" xmi:id="-GctCnxRLNQrvF3xEww8Egw" name="system_overview,_fuiWoP27EeK8HINtOGo1Xg" guid="-GctCnxRLNQrvF3xEww8Egw">
