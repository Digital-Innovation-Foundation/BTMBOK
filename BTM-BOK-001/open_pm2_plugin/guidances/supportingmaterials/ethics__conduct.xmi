<org.eclipse.epf.uma:ContentDescription xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:org.eclipse.epf.uma="http://www.eclipse.org/epf/uma/1.0.6/uma.ecore" xmlns:epf="http://www.eclipse.org/epf" epf:version="1.5.1" xmi:id="-8r4UYsXDAFCYv-jg1xwrTA" name="new_supporting_material,_2vjB8LieEemyQ55KauwYTg" guid="-8r4UYsXDAFCYv-jg1xwrTA" changeDate="2019-08-06T07:09:06.000-0400">
