  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-5o1LKLURgXXdeHz369WtUA" name="ea_review_AD_cio,_RlBfQMhVEdyU89e0dKAsrw" guid="-5o1LKLURgXXdeHz369WtUA">
    <usageGuidance>The AD is deemed to be an enterprise-level decision, therefore this review group will approve/reject the decision.&#xD;
