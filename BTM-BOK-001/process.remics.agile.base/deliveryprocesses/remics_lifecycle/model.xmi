      <processElements xsi:type="org.eclipse.epf.uma:WorkProductDescriptor" xmi:id="_U1BHQQvQEeOhutvlRKZMkg" name="project_plan" guid="_U1BHQQvQEeOhutvlRKZMkg" presentationName="Project Plan" briefDescription="The project plan describes the approach to migrate the legacy system into the new system." isPlanned="false" superActivities="_1kHlwTO7EeK5hZv7WCtlvg" WorkProduct="_YAkHIN_nEeKdMZ3N0jsS2w">
