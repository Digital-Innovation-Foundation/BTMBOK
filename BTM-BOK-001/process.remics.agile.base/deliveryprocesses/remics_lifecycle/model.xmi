      <processElements xsi:type="org.eclipse.epf.uma:RoleDescriptor" xmi:id="_6c-zceloEeKXeuh9rEjGtw" name="requirements_team" guid="_6c-zceloEeKXeuh9rEjGtw" presentationName="Requirements Team" briefDescription="The scrum team focussed in the identification of new requirements" isPlanned="false" superActivities="_xsG88TO8EeK5hZv7WCtlvg" Role="_Gi9t0F5AEeKVU8eUCvi3Og" responsibleFor="_UinvsQvQEeOhutvlRKZMkg _UiZGMQvQEeOhutvlRKZMkg _Uk3ycQvQEeOhutvlRKZMkg">
