        <processElements xsi:type="org.eclipse.epf.uma:TaskDescriptor" xmi:id="_mISG8P27EeK8HINtOGo1Xg" name="identify_interoperability_problems_scenarios" guid="_mISG8P27EeK8HINtOGo1Xg" presentationName="Identify interoperability problems/scenarios" superActivities="_BElx0f27EeK8HINtOGo1Xg" linkToPredecessor="_OwqOUP29EeK8HINtOGo1Xg" Task="_WdUNwVjGEeKc_ty_5LPMaw" mandatoryInput="_fuiWoP27EeK8HINtOGo1Xg _Cdl6Mv29EeK8HINtOGo1Xg" optionalInput="_Cdi25P29EeK8HINtOGo1Xg" output="_mjKFFf27EeK8HINtOGo1Xg" performedPrimarilyBy="_mISG8f27EeK8HINtOGo1Xg">
